{
  "name": "voyager-service",
  "module": "index.ts",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "bun run server:start",
    "server:build": "ncc build ./src/server.ts -o ./dist --source-map --transpile-only && bun run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org metorial --project mte-voyager ./dist && sentry-cli sourcemaps upload --org metorial --project mte-voyager ./dist",
    "server:start": "bun ./dist/index.js",
    "dev": "docker compose -p voyager -f ./docker-compose.dev.yml up --pull always --build voyager"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.3",
    "@vercel/ncc": "^0.38.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.957.0",
    "@aws-sdk/client-iam": "^3.957.0",
    "@aws-sdk/client-lambda": "^3.958.0",
    "@lowerdeck/api-mux": "^1.0.2",
    "@lowerdeck/cache": "^1.0.1",
    "@lowerdeck/canonicalize": "^1.0.4",
    "@lowerdeck/cron": "^1.0.7",
    "@lowerdeck/encryption": "^1.0.7",
    "@lowerdeck/env": "^1.0.4",
    "@lowerdeck/error": "^1.1.0",
    "@lowerdeck/hash": "^1.0.4",
    "@lowerdeck/id": "^1.0.6",
    "@lowerdeck/once": "^1.0.4",
    "@lowerdeck/pagination": "^1.0.5",
    "@lowerdeck/queue": "^1.0.7",
    "@lowerdeck/rpc-client": "^1.1.0",
    "@lowerdeck/rpc-server": "^1.0.7",
    "@lowerdeck/service": "^1.0.3",
    "@lowerdeck/slugify": "^1.0.5",
    "@lowerdeck/sentry": "^1.0.2",
    "@lowerdeck/validation": "^1.0.4",
    "@metorial-services/forge-client": "^1.0.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/bun": "^10.38.0",
    "@sentry/cli": "^3.2.0",
    "@types/pg": "^8.16.0",
    "algoliasearch": "^5.48.1",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "ipaddr.js": "^2.3.0",
    "jszip": "^3.10.1",
    "object-storage-client": "^0.2.5",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "prisma-json-types-generator": "^4.0.1",
    "snowflake-uuid": "^1.0.0",
    "yaml": "^2.8.2"
  }
}
